#include<iostream>
using namespace std;
template<class T>

class Ascending
{
	public:
		void sort()
 {
		int i,j,n;
		T temp;

      cout<<"Enter the amount of number:";
     cin>>n;
     T k[n];
     for(i=0;i<=n;i++)
     {

		cin>>k[i];
	
}
for(i=0;i<=n;i++)
{
	for(j=i+1;j<=n;j++)
	{
		if(k[i]>k[j])
		{
			temp=k[i];
			k[i]=k[j];
			k[i]=temp;
		}
	}


}
for(i=0;i<=n;i++)
{
	cout<<k[i];
	cout<<endl;
}	
		
}
	
};


int main()
{
Ascending<int> obj;
obj.sort();


return 0;
}
